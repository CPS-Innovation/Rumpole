{
  "name": "rumpole-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/instrument-cra": "^1.4.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.0.5",
    "@mui/material": "^5.0.6",
    "@mui/system": "^5.0.6",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^12.1.10",
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.3.2",
    "cypress": "^8.7.0",
    "date-fns": "^2.25.0",
    "env-cmd": "^10.1.0",
    "faker": "^5.5.3",
    "husky": ">=6",
    "istanbul-lib-coverage": "^3.2.0",
    "lint-staged": ">=10",
    "msw": "^0.35.0",
    "ncp": "^2.0.0",
    "nyc": "^15.1.0",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "xunit-viewer": "^7.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "prettier:check": "prettier --check .",
    "cy:dev": "start-server-and-test 'env-cmd -f .env.cypress react-scripts -r @cypress/instrument-cra start' http-get://localhost:3000 'env-cmd -f .env.cypress cypress open'",
    "cy:build": "CYPRESS_INSTRUMENT_PRODUCTION=true env-cmd -f .env.cypress react-scripts  build && ncp build build-e2e",
    "cy:ci": "cypress verify && cypress info && start-server-and-test 'serve -l 3000 -s build-e2e' http-get://localhost:3000 'env-cmd -f .env.cypress cypress run'",
    "cy:report": "npx xunit-viewer -r report -o report/index.html",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "nyc": {
    "report-dir": "coverage-cypress"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "devDependencies": {
    "prettier": "2.4.1"
  }
}
